<!DOCTYPE html>
<html>
<head>
    <title>Wifi System {% block title %}{% endblock %}</title>
    <!-- STYLESHEETS --><!--[if lt IE 9]><script src="js/flot/excanvas.min.js"></script>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <script src="http://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script><![endif]-->
    <link rel="stylesheet" href='/bundles/advertiser/assets/login/css/cloud-admin.css' />
    <link rel="stylesheet" href='/bundles/advertiser/assets/login/css/font-awesome.min.css' />
    <link rel="stylesheet" href='/bundles/advertiser/assets/login/css/daterangepicker-bs3.css' />
    <link rel="stylesheet" href='/bundles/advertiser/assets/login/uniform.default.min.css' />
    <link rel="stylesheet" href='/bundles/advertiser/assets/login/css/animate.min.css' />

    <script src="/bundles/advertiser/assets/login/jquery-2.0.3.min.js"></script>
    {% block stylesheets %}{% endblock %}
</head>
<body class="login">
    <!-- PAGE -->
    <section id="page">
        <!-- HEADER -->
        <header>
            <!-- NAV-BAR -->
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-md-offset-4">
                        <div id="logo">
                            <a href="http://wiads.vn">
                                {#
                                {% image '/bundles/advertiser/assets/login/img/logowiads300.png' %}
                                <img src="{{ asset_url }}" width="300" alt="logo name">
                                {% endimage %}
                                #}
                                <img src="/bundles/advertiser/assets/login/img/logowiads300.png"/>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <!--/NAV-BAR -->
        </header>
        <!--/HEADER -->
        {% block content %}{% endblock %}
    </section>
    <!--/PAGE -->
    <script src="/bundles/advertiser/assets/login/jquery-ui-1.10.3.custom.min.js"></script>
    <script src="/bundles/advertiser/assets/js/bootstrap.min.js"></script>
    <script src="/bundles/advertiser/assets/login/jquery.uniform.min.js"></script>
    <script src="/bundles/advertiser/assets/login/script.js"></script>
    {% block javascript %}{% endblock %}
</body>
</html>