<!DOCTYPE HTML>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<meta charset=utf-8>
  <title>{{ title }}</title>
  <meta http-equiv="Cache-control" content="no-cache">
  <meta http-equiv="Pragma" content="no-cache">
  <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
	{% if uamsecret and userpassword %}
 <meta http-equiv="refresh" content="1;url=http://{{uamip}}:{{uamport}}/logon?username={{username}}&password={{pappassword}}&userurl={{userurl}}">
  	{% else %}
    	<meta http-equiv="refresh" content="1;url=http://{{uamip}}:{{uamport}}/logon?username={{username}}&response={{response}}&userurl={{userurl}}">
	{% endif %}

    <link rel="stylesheet" href="/bundles/hotspotaccesspoint/act/dist/css/bootstrap.min.css" type="text/css">  
    <link rel="stylesheet" href="/bundles/hotspotaccesspoint/act/css/custom.css"/>
    <link rel="stylesheet" href="/bundles/hotspotaccesspoint/act/css/responsive.css"/>	
    <script>
      window.fbAsyncInit = function() {
        FB.Event.subscribe(
          'ad.loaded',
          function(placementId) {
            console.log('Audience Network ad loaded');
          }
        );
        FB.Event.subscribe(
          'ad.error',
          function(errorCode, errorMessage, placementId) {
            console.log('Audience Network error (' + errorCode + ') ' + errorMessage);
          }
        );
      };
      (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_US/sdk/xfbml.ad.js#xfbml=1&version=v2.5&appId=1206356572726483";
        fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));
</script>
</head
<body>
<body class="v2016 option2">
    <section id="main-content" class="clearfix">
		<div id="home-ads-list" class="container">
			<div class="row">
                <div class="col-md-12 text-center">					
					<div class="ads2-item">
					<p style="font-size: 14px;">
						Đang kết nối, vui lòng chờ ...
					</p>
					</div>
				</div>
			</div>
			<!-- facebook banners -->
			<div class="row">
                <div class="col-md-12 text-center">	
					<div class="fb-ad" data-placementid="1206356572726483_1282644068431066" data-format="320x50" data-testmode="false"></div>
					<div class="ads2-item"></div>
				</div>
            </div>			
		</div>
    </section>
</body>
</html>