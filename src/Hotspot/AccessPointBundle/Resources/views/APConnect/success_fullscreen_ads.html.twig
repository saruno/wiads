<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    {#% if 'facebook.com' not in linkTo and linkTo is not empty %#}
    {% if linkTo is not empty %}
        <meta http-equiv="refresh" content="0;url={{ linkTo | raw }}">
    {% endif %}
    <title>Success</title>
    <link rel="stylesheet" href='/bundles/advertiser/assets/css/demo12.css' />
</head>
<body>
<div class="container">
    <div class="screen">
        <header>
            <div class="head">
                {% if params['apUrl'] is empty %}
                    <a href="{{ params['apUrl'] }}" target="_blank"><p>Kết nối thành công</p></a>
                {% else %}
                    <a href="{{ linkTo | raw }}" target='_blank'><p>Kết nối thành công</p></a>
                {% endif %}
            </div>
        </header>
        <div class="wrap_connect">
            {% if ads is not empty %}
                <div>
                    <!--
                  No count
                  <a href="{{linkTo}}"><img class=""  src="/ap/display2.html?id={{ ads[0].Id }}&link={{ ads[0].linkTo | url_encode}}&called={{ params['called'] }}&mac={{ params['mac'] }}&uamip={{ params['uamip'] }}&ip={{ params['ip'] }}&userurl={{params['userurl']|url_encode}}" alt="image" /></a>
                  -->
                    <a href="{{linkTo}}"><img class=""  src="/ap/display.html?id={{ ads[0].Id }}&link={{ ads[0].linkTo | url_encode}}&called={{ params['called'] }}&mac={{ params['mac'] }}&uamip={{ params['uamip'] }}&ip={{ params['ip'] }}&userurl={{params['userurl']|url_encode}}" alt="image" /></a>
                </div>
            {% else %}
                <img src="/bundles/advertiser/assets/img/bg.png" alt="BANNER">
            {% endif %}
        </div>

    </div>
</div>
</body>
</html>