<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href='/bundles/advertiser/assets/css/wifi_login.css' />
    <script src="/bundles/advertiser/assets/js/jquery-1.11.0.min.js"></script>
    <script>
    function Login(value)
	{
		$('#advertId').val(value);
		$( "#login_frm" ).submit();
	}
	</script>
</head>
<body onload="changeScreenSize()">
<div class="container">
    <header>
        <div class="head">
            {% if params['apName'] is not empty %}
                {% if params['apImage'] is not empty %}
                    <a href="javascript:Login('-2');">
                        <h3>{{ params['apName'] }}&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</h3>
                        <img src="{{ params['apImage'] }}" alt="{{ params['apName'] }}">
                    </a>
                {% else %}
                    <a href="javascript:Login('-2');">
                        <h3>{{ params['apName'] }}</h3>
                        <div class="logo_wiads">
                            <img src="/bundles/advertiser/assets/img/logo_wiads.png" alt="LOGO WIADS">
                        </div>
                        <!--
                        <div class="logo_accesspoint" style="background: url('/bundles/advertiser/assets/img/logo_accesspoint.png') no-repeat center center / auto 100%"></div>
                        -->

                        <img src="/bundles/advertiser/assets/img/banner_640x152.jpg" alt="640x152">
                    </a>
                {% endif %}
            {% else %}
                <a href="javascript:Login('-2');">
                    <div class="logo_wiads">
                        <img src="/bundles/advertiser/assets/img/logo_wiads.png" alt="LOGO WIADS">
                    </div>
                    <!--
                    <div class="logo_accesspoint" style="background: url('/bundles/advertiser/assets/img/logo_accesspoint.png') no-repeat center center / auto 100%"></div>
                    -->
                    <img src="/bundles/advertiser/assets/img/banner_640x152.jpg" alt="640x152">
                </a>
            {% endif %}
        </div>
    </header>
    <section>
        <div class="box_a">
            {% if ads['QA0_v4'] is not empty %}
                <a href="javascript:Login('{{ ads['QA0_v4'].Id }}');">
                    <img class=""  src="/ap/display.html?id={{ ads['QA0_v4'].Id }}&link={{ ads['QA0_v4'].linkTo | url_encode}}&position=QA0_v4&called={{ params['called'] }}&mac={{ params['mac'] }}&uamip={{ params['uamip'] }}&ip={{ params['ip'] }}&userurl={{params['userurl']|url_encode}}" alt="image" />
                </a>
            {% else %}
                <a href="javascript:Login('-1');">
                    <img src="/bundles/advertiser/assets/img/QA0_v4_300x600.jpg">
                </a>
            {% endif %}
        </div>
        <div class="box_b">
            <div class="box_bb box_b1">
                {% if ads['QA1_v4'] is not empty %}
                    <a href="javascript:Login('{{ ads['QA1_v4'].Id }}');">
                        <img class=""  src="/ap/display.html?id={{ ads['QA1_v4'].Id }}&link={{ ads['QA1_v4'].linkTo | url_encode}}&position=QA1_v4&called={{ params['called'] }}&mac={{ params['mac'] }}&uamip={{ params['uamip'] }}&ip={{ params['ip'] }}&userurl={{params['userurl']|url_encode}}" alt="image" />
                    </a>
                {% else %}
                    <a href="javascript:Login('-1');">
                        <img src="/bundles/advertiser/assets/img/QA1_v4_336x280.jpg">
                    </a>
                {% endif %}
            </div>
            <div class="box_bb box_b2">
                {% if ads['QA2_v4'] is not empty %}
                    <a href="javascript:Login('{{ ads['QA2_v4'].Id }}');">
                        <img class=""  src="/ap/display.html?id={{ ads['QA2_v4'].Id }}&link={{ ads['QA2_v4'].linkTo | url_encode}}&position=QA2_v4&called={{ params['called'] }}&mac={{ params['mac'] }}&uamip={{ params['uamip'] }}&ip={{ params['ip'] }}&userurl={{params['userurl']|url_encode}}" alt="image" />
                    </a>
                {% else %}
                    <a href="javascript:Login('-1');">
                        <img src="/bundles/advertiser/assets/img/QA2_v4_336x280.jpg">
                    </a>
                {% endif %}
            </div>
        </div>
        <div class="box_d">
            {% if ads['QA3_v4'] is not empty %}
                <a href="javascript:Login('{{ ads['QA3_v4'].Id }}');">
                    <img class=""  src="/ap/display.html?id={{ ads['QA3_v4'].Id }}&link={{ ads['QA3_v4'].linkTo | url_encode}}&position=QA3_v4&called={{ params['called'] }}&mac={{ params['mac'] }}&uamip={{ params['uamip'] }}&ip={{ params['ip'] }}&userurl={{params['userurl']|url_encode}}" alt="image" />
                </a>
            {% else %}
                <a href="javascript:Login('-1');">
                    <img src="/bundles/advertiser/assets/img/QA3_v4_300x250.jpg">
                </a>
            {% endif %}
        </div>
        <div class="box_c">
            <div class="box_c1">
                <form class="login-form" novalidate="novalidate" id='login_frm' name='login_frm' method='post' action='{{params['loginpath']}}'>
                    <input type='hidden' name='called' value='{{params['called']}}'>
                    <input type='hidden' name='mac' value='{{params['mac']}}'>
                    <input type='hidden' name='challenge' value='{{params['challenge']}}'>
                    <input type='hidden' name='uamip' value='{{params['uamip']}}'>
                    <input type='hidden' name='ip' value='{{params['ip']}}'>
                    <input type='hidden' name='uamport' value='{{params['uamport']}}'>
                    <input type='hidden' name='userurl' value='{{params['userurl']|url_encode}}'>
                    <input type='hidden' id='advertId' name='advertId' value='-1'>
                    <input type='hidden' name='button' value='Free Internet'>
                </form>
                <a href="javascript:Login('-1');">
                    <img src="/bundles/advertiser/assets/img/login_335x100.jpg">
                </a>
            </div>
            <div class="box_c2">
                {% if ads['QA4_v4'] is not empty %}
                    <a href="javascript:Login('{{ ads['QA4_v4'].Id }}');">
                        <img class=""  src="/ap/display.html?id={{ ads['QA4_v4'].Id }}&link={{ ads['QA4_v4'].linkTo | url_encode}}&position=QA4_v4&called={{ params['called'] }}&mac={{ params['mac'] }}&uamip={{ params['uamip'] }}&ip={{ params['ip'] }}&userurl={{params['userurl']|url_encode}}" alt="image" />
                    </a>
                {% else %}
                    <a href="javascript:Login('-1');">
                        <img src="/bundles/advertiser/assets/img/login_tks_335x114.jpg">
                    </a>
                {% endif %}
            </div>
        </div>
        <div class="box_e">
            <a href="#"><img src="/ap/track_login.html?called={{ params['called'] }}&mac={{ params['mac'] }}&challenge={{ params['challenge'] }}&ip={{ params['ip'] }}&userurl={{params['userurl']|url_encode}}"/></a>
        </div>
    </section>
</div>
<script type="text/javascript" charset="utf-8" async defer>
    jQuery(document).ready(function() {


    });
    function changeScreenSize(){
        var wi = $("section").width();
        if(parseInt(wi) < 640){
            var section = $("section").height();
            var box1 = $("div.box_b").height();
            var box2 = $("div.box_d").height();

            var cu = parseInt(section) - ( parseInt(box1) + parseInt(box2) + 6 );

            var box_e = $("div.box_e");
            //box_e.css({"height":cu,"float":"left","width":"50%","background":"#a1a1a0"});

            //

            var a2 = parseInt(box1)+ 30 ;
            $("div.box_a").css({"height":a2+"px"});

            var x = (30*100)/wi; var hai_px = (2*100)/wi;
            var c = 50+x-hai_px;
            var d= 50-x;
            $("div.box_c").css({"width":c+"%","margin-right":hai_px+"%"});
            $("div.box_d").css({"width":d+"%"});

            var box_d = $("div.box_d").height();
            var bt = box_d - 30;
            $("div.box_c").css({"height":bt+"px"});

            $("div.box_c1").css({"height":"50%"});
            $("div.box_c2").css({"height":"50%"});

        }
    }

    window.onresize = function(event) {
        changeScreenSize();
    };
</script>
</body>
</html>